<% if error.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= t("text.text")%><%= pluralize(error.errors.count, "error") %>.
    </div>
    <ul>
    <% error.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
